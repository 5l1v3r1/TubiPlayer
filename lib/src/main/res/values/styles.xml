<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TubiPlayerTheme" parent="android:Theme.Holo">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:keepScreenOn">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="TubiSeekBar" parent="@android:style/Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/view_tubi_player_controller_seek_bar</item>
        <item name="android:thumb">@drawable/view_tubi_player_controller_seek_bar_thumb</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:maxHeight">2dp</item>
    </style>

</resources>